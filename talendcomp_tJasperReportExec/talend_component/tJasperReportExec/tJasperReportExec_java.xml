			NUM_ROW="60" 
			GROUP="OPTIONS_XLS_XLSX"
			SHOW_IF="(OUTPUT_TYPE == 'XLS') or (OUTPUT_TYPE == 'XLSX')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

